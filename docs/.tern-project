{
  "ecmaVersion": 7,
  "libs": [
    "browser",
    "jquery"
  ],
  "loadEagerly": [
    "**/*.js"
  ],
  "dontLoad": [
    "node_modules/**",
    "**/*.js"
  ],
  "plugins": {
    "modules": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
